{
  "version": 2,
  "builds": [
    { "src": "music_controller_app", "use": "@vercel/python", "config": { "maxLambdaSize": "1024mb", "python": "3.9" } }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
